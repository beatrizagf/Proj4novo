				}, 
				{
					"CurrentState": "S2",
					"NextState": "S3",
					"Meaning": "-",
					"Style": "Neutral",
					"Utterance": "Estás um bocado pálido... Sentes-te bem?",
					"UtteranceId": "TTS-33A616F98FAE27D99782E9705602DAE1"
				}, 
				{
					"CurrentState": "S5",
					"NextState": "S6",
					"Meaning": "-",
					"Style": "-",
					"Utterance": "Está tudo bem... Mas porque é que me mataste, amigo?",
					"UtteranceId": "TTS-69D165706051246DC28D06FF38EDC2E0"
				}, 
				{
					"CurrentState": "S7",
					"NextState": "S8",
					"Meaning": "-",
					"Style": "Sad",
					"Utterance": "Que falta de chá!",
